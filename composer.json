{
    "name": "serv-tec\/composer-patches",
    "version": "3.5.1",
    "description": "Provides a way to patch Composer packages with optional version restriction",
    "keywords": [
        "composer plugin",
        "patching",
        "version restriction",
        "indirect restrictions",
        "patch description",
        "remote patch files",
        "downloaded patches",
        "package bug-fix",
        "package patches",
        "patch exclusion",
        "development patches",
        "hot-fixes"
    ],
    "support": {
        "issues": "https:\/\/github.com\/serv-tec\/Composer-Patches\/issues",
        "wiki": "https:\/\/github.com\/serv-tec\/Composer-Patches\/wiki",
        "source": "https:\/\/github.com\/serv-tec\/Composer-Patches"
    },
    "minimum-stability": "dev",
    "license": "BSD",
    "type": "composer-plugin",
    "extra": {
        "class": "Vaimo\\ComposerPatches\\Plugin"
    },
    "authors": [
        {
            "name": "Cameron Eagans",
            "email": "me@cweagans.net"
        },
        {
            "name": "Allan Paiste",
            "email": "allan.paiste@vaimo.com"
        },
        {
            "name": "serv-tec",
            "homepage": "https:\/\/github.com\/serv-tec",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer\/composer": "~1.0",
        "phpunit\/phpunit": "~4.6"
    },
    "conflict": {
        "cweagans\/composer-patches": "*"
    },
    "autoload": {
        "psr-4": {
            "Vaimo\\ComposerPatches\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vaimo\\ComposerPatches\\Tests\\": "tests"
        }
    },
    "old": {
        "owner": "vaimo",
        "name": "composer-patches"
    },
    "replace": {
        "vaimo\/composer-patches": "*"
    }
}